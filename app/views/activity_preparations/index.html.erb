<p id="notice"><%= notice %></p>

<h1>Activity Preparations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_preparations.each do |activity_preparation| %>
      <tr>
        <td><%= activity_preparation.name %></td>
        <td><%= link_to 'Show', activity_preparation %></td>
        <td><%= link_to 'Edit', edit_activity_preparation_path(activity_preparation) %></td>
        <td><%= link_to 'Destroy', activity_preparation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Preparation', new_activity_preparation_path %>
