<p id="notice"><%= notice %></p>

<h1>Activity Others</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @activity_others.each do |activity_other| %>
      <tr>
        <td><%= activity_other.name %></td>
        <td><%= link_to 'Show', activity_other %></td>
        <td><%= link_to 'Edit', edit_activity_other_path(activity_other) %></td>
        <td><%= link_to 'Destroy', activity_other, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Activity Other', new_activity_other_path %>
